{
	"name": "utimes",
	"version": "4.1.1",
	"description": "Native addon to change the btime, mtime, and atime of a file on Windows, macOS, and Linux.",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"binding.gyp",
		"dist/**/*",
		"cpp/**/*"
	],
	"homepage": "https://github.com/baileyherbert/utimes",
	"repository": "github:baileyherbert/utimes",
	"bugs": "https://github.com/baileyherbert/utimes/issues",
	"author": "Bailey <hello@bailey.sh> (https://github.com/baileyherbert)",
	"contributors": [
		"Joran Dirk Greef (https://github.com/jorangreef)",
		"Jule (https://github.com/Jule-)"
	],
	"keywords": [
		"access time",
		"atime",
		"birth time",
		"btime",
		"created time",
		"fs",
		"modified time",
		"mtime",
		"utimes"
	],
	"license": "MIT",
	"scripts": {
		"install": "node-pre-gyp install --fallback-to-build && node-gyp clean",
		"install:from-remote": "node-pre-gyp install --fallback-to-build=false --update-binary",
		"build": "node-pre-gyp clean configure build package",
		"publish:x64": "node-pre-gyp clean configure build package publish --target_arch=x64",
		"publish:x86": "node-pre-gyp clean configure build package publish --target_arch=ia32",
		"publish:arm": "node-pre-gyp clean configure build package publish --target_arch=arm",
		"publish:clean": "node-pre-gyp clean && node-gyp clean",
		"unpublish": "node-pre-gyp unpublish",
		"tsc": "rimraf dist && tsc",
		"test": "jest"
	},
	"dependencies": {
		"@mapbox/node-pre-gyp": "^1.0.5",
		"node-addon-api": "^3.2.1"
	},
	"devDependencies": {
		"@types/jest": "^27.0.1",
		"@types/node": "^16.9.0",
		"aws-sdk": "^2.910.0",
		"jest": "^25.5.4",
		"rimraf": "^3.0.2",
		"ts-jest": "^25.5.1",
		"typescript": "^3.9.10"
	},
	"binary": {
		"module_name": "utimes",
		"module_path": "./artifacts/",
		"remote_path": "./{version}/{configuration}/",
		"package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
		"host": "https://node-utimes.s3.amazonaws.com"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"globals": {
			"ts-jest": {
				"tsconfig": "tests/tsconfig.json"
			}
		}
	}
}
